<section class="blog-card m-4">
  <div class="container-form">
    <form action="/organizaciones/registrar" class="formulario" id="formulario" method="POST">
      <div class="row">
        <div class="col-md-6">
          <h4 class="tituloColumna">Información de organización</h4>
          <div class="row itemInfo">
            <div class="col-md-3 col3">
              <label for="" class="labels">Nombre de la organización</label>
            </div>
            <div class="col-md-3 col3">
              <input type="text" class="input" name = "orgName" placeholder="Nombre de la organización">
            </div>
          </div>
          <div class="row itemInfo">
            <div class="col-md-3 col3">
              <label for="" class="labels">Dominio de la organización</label>
            </div>
            <div class="col-md-3 col3">
              <input type="text" class="input" name = "orgUrl" placeholder="Dominio de la organización">
            </div>
          </div>
          <div class="row itemInfo">
            <div class="col-md-3 col3">
              <label for="" class="labels">ACS URL</label>
            </div>
            <div class="col-md-3 col3">
              <input type="text" class="input" placeholder="ACS URL" name="ACSURL">
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <h4 class="tituloColumna">Contacto Técnico</h4>
          <div class="row itemInfo">
            <div class="col-md-3 col3">
              <label for="" class="labels">Nombre</label>
            </div>
            <div class="col-md-3 col3">
              <input type="text" class="input" placeholder="Nombre contacto técnico" name="nameTec">
            </div>
          </div>
          <div class="row itemInfo">
            <div class="col-md-3 col3">
              <label for="" class="labels">Correo Electrónico</label>
            </div>
            <div class="col-md-3 col3">
              <input type="text" class="input" placeholder="Correo Electrónico" name="emailTec">
            </div>
          </div>
        </div>
      </div>
      <div class="row itemInfo">
        <div class="col-md-2">
          <label for="principal" class="labels">
            Tiempo de proceso
          </label>
          </div>
        <div class="col-md-2">
          <input
            type="text"
            class="input"
            placeholder="Tiempo de proceso"
            name="timeProcess"
          />
        </div>
      </div>
      <div class="row">
        <div class="row bottons">
        <div class="col-md-5">
          <button
            id="clear"
            type="button"
            class="btn-soyyo"
            name="limpiarCampos"
          >
            Limpiar Campos
          </button>
        </div>
        <div class="col-md-5">
            <button type="button" id="addClient" form="formulario" class="btn-soyyo">Registrar</button>

          
        </div>
      </div>
      </div>


    </form>
  </div>
</section>
<script src="/formulariosaml.js"></script>

